		<div class="search-total-dim"></div>
		<!-- //퍼블 통합검색(HTML) -->

		<!-- //공통 : footer -->

		<script type="text/javascript">
			select.initialize();
			check.initialize();
		
		
		
			$("#btnSubmit").on('click', function (e) {
				fncValidate();
			});
		
			$('#paramInvcNo').keydown(function (e) {
				if(e.keyCode == 13){
					fncValidate();
				}
			});
		
			function fncValidate(){
				var paramInvcNo = $('#paramInvcNo').val();
				
				if (paramInvcNo != null && paramInvcNo != "") {
					if (paramInvcNo.byte() != 10 && paramInvcNo.byte() != 12) {
						alert("운송장 번호는 12자리 또는 10자리입니다.");
						$('#paramInvcNo').focus();
						return false;
					}
				}else{
					alert("운송장번호를 입력하세요");
					$('#paramInvcNo').focus();
					return false;
				}
			
				$.ajax({
					url: '/ko/tool/parcel/tracking-detail',
					type: 'POST',
					dataType: 'json',
					data: GLOBAL_CSRF_NAME + '=' + GLOBAL_CSRF_VALUE + '&paramInvcNo=' + paramInvcNo,
					timeout: 1000,
					async: false,
					error: function(xhr){
						if(xhr.status == "403"){
							location.href = location.href;
						}
					},
					success: fncCreateResult
				});
			}
		
			function fncCreateResult(responseText){
				if(responseText != null && responseText.parcelResultMap != null && responseText.parcelDetailResultMap != null){
					var parcelResultMap = responseText.parcelResultMap.resultList;
					var parcelDetailResultMap = responseText.parcelDetailResultMap.resultList;
		
					var paramInvcNo = responseText.parcelResultMap.paramInvcNo;
		
					var imageStatus = 1;
					var imageAlt = "";
					var statusDetail = "";
		
					var deliveryStatus = 0;
					var tmpInfo = "";
		
					var empName = "";
					var empPhone = "";
		
					var paramBranchCd = "";
					var paramEmpId = "";
		
					if (typeof responseText == 'undefined' || responseText == ''){
						alert('배송정보 조회 요청이 실패하였습니다.');
						return false;
					}else if((typeof parcelResultMap == 'undefined' || parcelResultMap == '') && (typeof parcelDetailResultMap == 'undefined' || parcelDetailResultMap == '')){
						$('#isResult').css('display','none');
						$('#deliveryDetail').css('display','none');
						$('#notResult').css('display','block');
		
					}else{
						$("#invcNo").text('-');
						$("#sendrNm").text('-');
						$("#rcvrNm").text('-');
						$("#itemNm").text('-');
						$("#qty").text('-');
						if(parcelResultMap.length > 0){
							var sendrNm = parcelResultMap[0].sendrNm;
							var rcvrNm = parcelResultMap[0].rcvrNm;
							var rgmailNo = parcelResultMap[0].rgmailNo;
							var rtnTrspbillnum = parcelResultMap[0].rtnTrspbillnum;
							var oriTrspbillnum = parcelResultMap[0].oriTrspbillnum;
		
							if(sendrNm.length > 1){
								sendrNm = sendrNm.substring(0, 1) + "*";
							}
							if(rcvrNm.length > 1){
								rcvrNm = rcvrNm.substring(0, 1) + "*";
							}
							if (rgmailNo != null && rgmailNo != '' && rgmailNo !='&nbsp;'){
								$("#invcNo").html(parcelResultMap[0].invcNo + '<br><br>(우체국위탁배송)<br><a href="http://service.epost.go.kr/trace.RetrieveRegiPrclDeliv.postal?sid1=' + rgmailNo + '" title="우체국 등기번호 조회" target="_blank" onclick=" window.open(this.href,\'popup\',\'left=0,top=0,width=1024,height=900,resizable=yes,status=yes,toolbar=no,menubar=no,scrollbars=yes\'); return false;"> <b><ins><font color=\"#007ac3\">우체국조회(' + rgmailNo + ')');
							}else{
								$("#invcNo").text(parcelResultMap[0].invcNo);
							}
		
							if(rtnTrspbillnum != null && rtnTrspbillnum != ''){
								$("#invcNo").append('</br></br>반품:' + rtnTrspbillnum + '</br>');
							}
							if(oriTrspbillnum != null && oriTrspbillnum != ''){
								$("#invcNo").append('</br></br>원운송장:' + oriTrspbillnum + '</br>');
							}
		
							$("#sendrNm").text(sendrNm);
							$("#rcvrNm").text(rcvrNm);
							$("#itemNm").text(parcelResultMap[0].itemNm);
							$("#qty").text(parcelResultMap[0].qty);
						}
		
						if(parcelDetailResultMap.length > 0){
							var langth = parcelDetailResultMap.length - 1;
							if(typeof parcelDetailResultMap[langth].crgSt == 'undefined' || parcelDetailResultMap[0].crgSt == ''){
								imageStatus = 1;
								imageAlt = '01. 상품준비';
							}else if(parcelDetailResultMap[langth].crgSt == '11'){
								imageStatus = 3;
								imageAlt = '03. 상품인수';
							}else if(parcelDetailResultMap[langth].crgSt == '41'){
								imageStatus = 4;
								imageAlt = '04. 상품이동중';
							}else if(parcelDetailResultMap[langth].crgSt == '42'){
								imageStatus = 5;
								imageAlt = '05. 배송지도착';
							}else if(parcelDetailResultMap[langth].crgSt == '44'){
								imageStatus = 4;
								imageAlt = '04. 상품이동중';
							}else if(parcelDetailResultMap[langth].crgSt == '82'){
								imageStatus = 6;
								imageAlt = '06. 배송출발';
							}else if(parcelDetailResultMap[langth].crgSt == '91'){
								imageStatus = 7;
								imageAlt = '07. 배달완료';
							}
							
							$("#statusImage").attr('src','/static/pc/global/template/images/img/img_step_info_' + imageStatus + '.jpg');
							$("#statusImage").attr('alt','상품 상태 확인 단계 - ' + imageAlt);
		
							for(i=0; i<parcelDetailResultMap.length; i++){
								var stautsName = "";
								if(parcelDetailResultMap[i].crgSt == '91'){
									statusDetail += "<tr class='end'>";
								}else{
									statusDetail += "<tr>";
								}
								if(parcelDetailResultMap[i].crgSt == '11'){
									stautsName = '상품인수';
								}else if(parcelDetailResultMap[i].crgSt == '41'){
									stautsName = '상품이동중';
								}else if(parcelDetailResultMap[i].crgSt == '42'){
									stautsName = '배송지도착';
								}else if(parcelDetailResultMap[i].crgSt == '44'){
									stautsName = '상품이동중';
								}else if(parcelDetailResultMap[i].crgSt == '82'){
									paramBranchCd = parcelDetailResultMap[i].regBranId;
									paramEmpId = parcelDetailResultMap[i].empImgNm;
		
									tmpInfo = parcelDetailResultMap[i].crgNm.split("배송담당:");
									if(tmpInfo.length == 2){
										var tmpInfo2 = (tmpInfo[1].replace(")", "")).split(" ");
										if(tmpInfo2.length == 2){
											empName = tmpInfo2[0];
											empPhone = tmpInfo2[1];
										}
									}
									
									stautsName = '배송출발';
									deliveryStatus = 1;
								}else if(parcelDetailResultMap[i].crgSt == '91'){
									stautsName = '배달완료';
								}
								
								statusDetail += "<td>" + stautsName + "</td>";
								statusDetail += "<td>" + parcelDetailResultMap[i].dTime + "</td>";
								statusDetail += "<td>" + parcelDetailResultMap[i].crgNm + "</td>";
								statusDetail += "<td>" + parcelDetailResultMap[i].regBranNm + "</td>";
								statusDetail += "<tr>";
		
							}
							$("#statusDetail").html(statusDetail);
						}else{
							statusDetail += "<tr>";
							statusDetail += "<td colspan='4' style='text-align:center;'>조회된 데이터가 없습니다.</td>";
							statusDetail += "<tr>";
							$("#statusDetail").html(statusDetail);
		
						}
		
						if(deliveryStatus == 1){
							$("#empName").text(empName);
							$("#empPhone").text(empPhone);
		
							$.ajax({
								url: '/ko/tool/parcel/tracking-deliveryDetail',
								type: 'POST',
								dataType: 'json',
								data: GLOBAL_CSRF_NAME + '=' + GLOBAL_CSRF_VALUE + '&paramBranchCd=' + paramBranchCd + '&paramEmpId=' + paramEmpId,
								timeout: 1000,
								async: false,
								error: function(xhr){
									if(xhr.status == "403"){
										location.href = location.href;
									}
								},
								success: function(responseText){
									if(responseText.deliveryResultMap != null){
										$("#belongName").text(responseText.deliveryResultMap.resultList[0].belongName);
										$("#subName").text(responseText.deliveryResultMap.resultList[0].subName);
										$("#branName").text(responseText.deliveryResultMap.resultList[0].branName);
										if(typeof responseText.fullPath != 'undefined' && responseText.fullPath != ''){
											$("#filepathName").attr('src', responseText.fullPath);
										}else{
											$("#filepathName").attr('src', '/static/pc/global/template/images/img/ko.jpg');
										}
									}else{
										alert('호출에 실패하였습니다.');
									}
								}
							});
							
							$('#deliveryDetail').css('display', 'block');
						}else{
							$('#deliveryDetail').css('display', 'none');
						}
						
						$('#notResult').css('display', 'none');
		
						//PC URL
					
						var cashbackUrl = "https://www.logii.com/interface/delvprom/delv_click.asp?prom_code=LA01&comp_code=korex&login_yn=N&login_name=&inv_no=" + paramInvcNo;
					
						$("#iptCashback").attr("href", cashbackUrl);
						$('#isResult').css('display', 'block');
		
					}
				}else{
					alert('서비스가 원활하지 않습니다.');
				}
			}
		
		</script>

























