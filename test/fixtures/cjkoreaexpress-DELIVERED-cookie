		<script type="text/javascript">
			function fncTrackingSearch(){
		var trackNum = "";
		var frm = document.frmTrackingSearch;
		trackNum = $('#trackingNum').val();
		
		
		
			
			
			if (trackNum != null && trackNum != "") {
				if (trackNum.byte() != 10 && trackNum.byte() != 12) {
					alert("운송장 번호는 12자리 또는 10자리입니다.");
					$('#trackingNum').focus();
					return false;
				}
			}else{
				alert("운송장번호를 입력하세요");
				$('#trackingNum').focus();
				return false;
			}
			frm.gnbInvcNo.value = trackNum;
		    frm.action = '/ko/tool/parcel/tracking';
		    frm.target="_self";
		    frm.method="POST";
		    frm.submit();
			
			
			
			
		
		
	};

	function fncGlobalCountryList(){
		var showYn1 = $('.box-globalLink.posi-2').hasClass('show');
		var showYn2 = $('#globalCountryList').find('li').length;
		if(!showYn1 && showYn2 == 0){
			//국가정보
			$.ajax({
				url: '/ko/network/country-list',
				type: 'POST',
				dataType: 'json',
				data: GLOBAL_CSRF_NAME+'='+GLOBAL_CSRF_VALUE,
				timeout: 1000,
				async: false,
				error: function(xhr){
					if(xhr.status=="403"){
						location.href = location.href;
					}
				},
				success: function(responseText){
					$("#globalCountryList").find("li").remove();
					var html = "";
					for(i=0; i<responseText.networkResult.length; i++){
						var detailUrl = responseText.networkResult[i].detailUrl;
				        detailUrl = '/ko/'+detailUrl.split('/')[2]+"/"+detailUrl.split('/')[3];
				        html += '<li><a href="'+detailUrl+'"><img class="img-flag" src="'+responseText.networkResult[i].flagImageUrl+'" alt="'+responseText.networkResult[i].flagFileMemo+'">'+responseText.networkResult[i].countryNm+'</a></li>';
					}
					$('#globalCountryList').html(html);
				}
			})
		}
	};
	
	function fncUnifiedSearch(){
		var frm = document.frmUnifiedSearch;
		
		var inputSearch = frm.elements["inputSearch"].value;
		if(inputSearch.trim() == null || inputSearch.trim() == '' ){
			alert('검색어를 입력하세요.');
			return false;
		}
		
		frm.action = '/ko/search';
	    frm.target="_self";
	    frm.method="POST";
		frm.submit();
	}
	
	function fncSuggestedSearch(_keyWord){
		$('.placeholder').prev('label').addClass("textup");
		var frm = document.frmUnifiedSearch;
		frm.elements["inputSearch"].value = _keyWord;
		fncUnifiedSearch();
	}
		</script>


