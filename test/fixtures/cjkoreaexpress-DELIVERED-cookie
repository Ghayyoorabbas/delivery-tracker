			<script type="text/javascript">
				$(document).ready(function() {
		// App 닫기
		$('#btn-closeApp').click(function(){
			$('.box-appSearch').slideUp();
		});
		
		// 엔터키 입력 시 SMS 발송
	    $('#receiver').keydown(function (e) {
			if(e.keyCode == 13){
				fncSendSMS();
	        }
		});
		
		// input type text 인 경우 clear 버튼 삽입
		$('.input-control input[type="text"]').on('input propertychange', function() {
			var $this = $(this);
			var visible = Boolean($this.val());
			$this.siblings('.form-control-clear').toggleClass('hidden', !visible);
		}).trigger('propertychange');

		$('.form-control-clear').click(function() {
			$(this).siblings('input[type="text"]').val('')
			.trigger('propertychange').focus();
		});
	});
	
	function fncSendSMS(){
		
		var receiver = null;
		receiver = $('#receiver').val();
		
		if(receiver.length < 1){
			alert("SMS를 받으실 휴대폰 번호를 입력 해 주세요.");
			$('#receiver').focus();
			return false;
		}
		
		// 휴대폰번호 유효성체크
		if(!receiver.isMobile()) {
			alert('SMS를 받으실 휴대폰 번호 형식을 확인해 주세요.');
			$('#receiver').focus();
			return false;
		}
		
		if(!$("input:checkbox[name=agreeSMSYn]").is(":checked")) {
			alert("개인정보 수집 및 이용에 동의해 주세요.");
			$('#agreeSMSYn').focus();
			return false;
		}
		

	
	
		$.ajax({
			url: '/ko/tool/parcel/sendSMS',
			type: 'POST',
			dataType: 'json',	
			data: GLOBAL_CSRF_NAME + '=' + GLOBAL_CSRF_VALUE + '&receiver=' + receiver,
			timeout: 1000,	
			async: false,		
			error: function(xhr){
				if(xhr.status == "403"){
					location.href = location.href;
				}
			},
			success: function(responseText){
				if(responseText != null && responseText.sendSMSResultMap != null && typeof responseText.sendSMSResultMap.resultList != 'undefined' && responseText.sendSMSResultMap.resultList != null){
					var fwdyn = responseText.sendSMSResultMap.resultList[0].FWDYN;
					var msg = '';
					
					if (fwdyn == 'D') {
						msg = '택배 APP 설치 안내 SMS가 이미 발송되었습니다. 휴대폰을 확인해 주시기 바랍니다.';
					} else {
						msg = 'SMS 발송이 완료되었습니다.';
					}
					
					alert(msg);
					
					$('#receiver').val('');
					$('#agreeSMSYn').siblings('label').removeClass('on');
					$('input[name*="agreeSMSYn"]').prop('checked', false);
					
				}else{
					alert('SMS 발송에 실패하였습니다.');
					$('#receiver').val('');
					$('#agreeSMSYn').siblings('label').removeClass('on');
					$('input[name*="agreeSMSYn"]').prop('checked', false);
				}
			}
		});
	

}
			</script>

