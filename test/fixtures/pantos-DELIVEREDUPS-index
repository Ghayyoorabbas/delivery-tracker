<script type="text/javascript" src="/resources/js/ecp/web/pr/dt/dlvChaseInq.js"></script>
<script type="text/javascript" language="javascript">
	var chaseCnt = 0;
	var quickNo;
	var locale;

	(function($, global){
		$(document).ready(function() {
			//화면 복사 방지
			document.oncontextmenu = new Function ('return false');
			document.ondragstart = new Function ('return false');
			document.onselectstart = new Function ('return false');
			document.body.style.MozUserSelect = 'none';
			quickNo = CommonUtil.filterXSS('');
			locale = CommonUtil.filterXSS('');

			if ( !cfIsNull(locale) ) {
				LOCALECD = locale;
			}

			$("#btnChaseSrch").click(listeners.onChaseSrchClick);
			$('#quickNo').keyup( function() {
				var quickNoLength = $.trim($(this).val()).split("\n").length;
				var allowedNumberOfLines = 30;
				if ( quickNoLength > allowedNumberOfLines ) {
					var modifiedText = $.trim($(this).val()).split("\n").slice(0, allowedNumberOfLines);
					$(this).val(modifiedText.join("\n"));
				}
			}).focusin(function() {
				document.oncontextmenu = null;
				document.ondragstart = null;
				document.onselectstart = null;
			}).focusout(function() {
				document.oncontextmenu = new Function ('return false');
				document.ondragstart = new Function ('return false');
				document.onselectstart = new Function ('return false');
			}).blur(function() {
				document.oncontextmenu = new Function ('return false');
				document.ondragstart = new Function ('return false');
				document.onselectstart = new Function ('return false');
			});
			fnInitChase();
		});

		var listeners = {
			onChaseSrchClick : function() {
				fnGetChaseList();
			}
		}

	})(jQuery, window);

	function fnInitChase() {
		if ( !cfIsNull(quickNo) ) {
			quickNo = quickNo.replace(/\n/g, "");
			$('#quickNo').val(quickNo);
			//fnGetChaseList();
		}
	}

	//배송추적목록조회
	function fnGetChaseList() {
		//var dataUrl = '/ecp/pr/dt/dlvchaseinq/retreiveDlvChaseList.dev';
		var dataUrl = '/ecp/pr/dt/dlvchaseinq/retreiveTrackingList.dev';
		var quickNoArr = new Array();



		if ( cfIsNull($.trim($('#quickNo').val())) ) {
			cfAlert('Required Input : [Inquiry No]');
			return;
		}
		var quickNo = $.trim($('#quickNo').val().toUpperCase());
		var quickNoList = quickNo.split("\n");

		var inputData = {
				IN_PARAM : {
					quickNo  : quickNoList
			      , locale : LOCALECD
				  //, pageToken : $("#pageToken").val()
				}
			};

		$("#chaseBody").html("");
		ajax({
			url : dataUrl,
			data : inputData,
			async : false,
			success : function(data) {
				if( data ){
					if( data.header ){
						if( data['result'] && data['result'].RESULT_CD != "0" ){
							cfAlert(data['result'].RESULT_MSG);
							return;
						}
						chaseCnt = data["OUT_DS1"].length;
						if( data["OUT_DS1"].length > 0 ){
							for ( var i = 0; i < data["OUT_DS1"].length; i++ ) {
								var chaseInfo = data["OUT_DS1"][i];
								fnAddChaseList(chaseInfo, i);
							}
						} else {
							cfAlert('If you can not track the delivery, please contact the vendor.');
							var chaseTr ="";
							//chaseTr +="<tr>";
							//chaseTr +="<td colspan=\"7\" class=\"nodata\"><ece:ez>gsi.msg.cm.noDataFound</ece:ez></td>";
							//chaseTr +="</tr>";
							$("#chaseBody").html(chaseTr);
						}
					}
				}
			}
		});
	}

	//배송추적상세조회
	function fnChaseDetail( hblNo, mblNo, expsBizTypeCd, carrSprCd, wmDomCarr, iframeAddrUrl, chaseId1, chaseId3 ) {
		//var dataUrl = '/ecp/pr/dt/dlvchaseinq/retreiveDlvChaseListDtl.dev';
		var dataUrl = '/ecp/pr/dt/dlvchaseinq/retreiveTrackingListDtl.dev';
		var inputData = {
				IN_PARAM : { hblNo : hblNo
					       , mblNo : mblNo
					       , locale : LOCALECD
					       , expsBizTypeCd : expsBizTypeCd
					       , carrSprCd : carrSprCd
					       , wmDomCarr : wmDomCarr
						   //, pageToken : $("#pageToken").val()
				}
		};
		var chaseTr1 = document.getElementById('chaseBody').getElementsByClassName('active')[0];
		var chaseDtlTr3 = document.getElementById(chaseId3);

		if ( ( !cfIsNull(chaseTr1) && chaseId1 == chaseTr1.id ) || chaseDtlTr3.hasChildNodes() ) {
			var curOpenYn = false;
			if ( "active" == document.getElementById(chaseId1).className ) {
				curOpenYn = true;
			}
			for ( var i = 0; i < chaseCnt; i++ ) {
				document.getElementById("chase1_"+i).removeAttribute("class");
			}
			if (!curOpenYn) {
				document.getElementById(chaseId1).setAttribute("class", "active");
			}
			return;
		}



		ajax({
			url : dataUrl,
			data : inputData,
			async : false,
			success : function(data) {
				if( data ){
					if( data.header ){
						if( data['result'] && data['result'].RESULT_CD != "0" ){
							cfAlert(data['result'].RESULT_MSG);
							return;
						}
						if( data["OUT_DS1"].length <= 0 ){
							cfAlert('If you can not track the delivery, please contact the vendor.');
						}
						//배송추적상세 처리
						fnOpenChaseDtl(data["OUT_DS1"], iframeAddrUrl, chaseId1, chaseId3);

					}
				}
			}
		});
	}

</script>

</html>
