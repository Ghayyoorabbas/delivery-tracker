<!-- Global Session Setting -->
<script type="text/javascript">
	var languageType = $.cookie('languageType');
    (function($, global) {
    	var url = window.location.href;
    	if ( url.indexOf("trackingPopup") > -1 || url.indexOf("dlvChaseInqPopup") > -1 ) {
    		if (window.location.protocol == 'https:') {
    	    	location.replace(url.replace("https:", "http:"));
    	    }
    	}

     	// 로그인 사용자 정보 설정
        sfnSetSessionInfo(GFN_SESSION_LIST);

        $(document).ready(function() {

	    	var setMenu = function(lang){
	           var menuUrl = "/resources/json/ecp/web-menu-"+lang+".json";
	           $.ajax({
					url: menuUrl,
					data: {},
					dataType: 'json',
					success: function(data) {
						buildUL(data);
					},
					error: function(xhr, status, error) {
						console.log(error);
					}
				});
	    	}
	    	var changePath = function(lang){
	     		var pathname = window.location.pathname; // Returns path only
	        	var path = pathname.substr(0, (pathname.lastIndexOf('/') +1) );
	        	var orgFileName  = pathname.split('/').pop().split('.').shift();
	        	var fileName =  orgFileName.substr(0, (orgFileName.lastIndexOf('-') ) );
	        	var langFileName = '';

	        	if ( lang== undefined || lang=='') { //기본 URL
	        		lang = "en";
	        		langFileName = fileName+'-'+lang;   // XXX-cn => XXX-en
	        	}

	        	setMenu(lang);

	        	if(lang=='ko'){                 // 한국어 언어설정인경우
	        		if(orgFileName.lastIndexOf('-')!=-1){   // 현재 url이 다른언어페이지 XXX-en.do, XXX-cn.do
	        			langFileName = fileName;            // => XXX.do
	        		}else{
	        			if(lang=='ko'){
	        				langFileName = orgFileName;     // => XXX.do
	        			}
	        		}
	        	}else{                                      // 영어 중국어 언어설정인경우
	        		if(orgFileName.lastIndexOf('-')!=-1){
	        			langFileName = fileName+'-'+lang;   // XXX-cn => XXX-en
	        		}else{
	        			langFileName = orgFileName+'-'+lang;  // XXX => XXX-en
	        		}
	        	}
	        	if(pathname.lastIndexOf('system')!=-1){
		        	if(orgFileName != langFileName && langFileName !=""){
		        		var langUrl = path + langFileName +'.do';
		    			window.location.href=langUrl;   // main.do => main-cn.do
		        	}
	    		}
	     	}

	    	if(_.isUndefined($.cookie('languageType'))){
	    		languageType = 'en';
	    	}else{
	    		languageType = $.cookie('languageType');
	    	}

	     	// 언어 변경에 따른 페이지 전환
	    	changeLang = function (lang) {
	    		if ( languageType != lang ) {
	    			$.cookie('languageType', lang,  { path : '/' });
		    		changePath(lang);
	        		var pathname = window.location.pathname; // Returns path only
	        		if( pathname.lastIndexOf('/pr/') > -1 ) {
	        			location.reload();
	        		}
	    		}
	        };


	        changePath(languageType);

	    	var buildUL = function (data) {
	    		var menu = $("#uiNavGnb > ul");
	        	menu.empty();
	        	_.each(data,function(item){
	        		var li = $("<li><a href="+item.url+">" + item.menuName + "</a></li>");
	                li.appendTo(menu);
	                if(_.isObject(item.subMenu)){
	                	var ul = $("<ul></ul>");
	                	ul.appendTo(li);
	                	_.each(item.subMenu,function(sub){
	                		var sli = $("<li><a href="+sub.url+">" + sub.menuName + "</a></li>");
	                		sli.appendTo(ul);
	                	});
	                }
	        	});
	        	ui.navGnb();
	    	}

	    	//업무별 연락처 링크
	    	$('#btnContactUs').click(function(){
	    		var currUrl = location.href;
	    		if ( currUrl.indexOf("contactInfo") > -1 ) {
	    			return;
	    		}
	    		var trankingUrl = '/ecp/web/cm/system/contactInfo.do';
	    		if ( languageType != undefined && languageType != '' && languageType != 'ko' ) {
	    			trankingUrl = '/ecp/web/cm/system/contactInfo-'+languageType+'.do';
	    		}
	    		location.href = trankingUrl;
	     	});

		});
    })(jQuery, window);


</script>

