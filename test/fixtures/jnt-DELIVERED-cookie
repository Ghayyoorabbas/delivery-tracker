<script>
    var notrobot = false;
    function robotVerified(){
        clientPC.invoke('user_yzCode', [{fyzm:$('#webyzm').val()}]).then(function (ret) {
            if(ret){
                notrobot = true;
                searchGZ();
                layer.closeAll();
            }else{
                layer.msg("Vercode Incorrect", {icon: 5,time:layuimsgtime});
            }
        })
    }
    function expVerified(){
        notrobot = false;
        $('#imgVercode').attr('src', '/captcha.html');
        $("input[name='webyzm']").val('');
    }
    function showrecaptcha(){
        $('.recaptcha_box').show();
        layer.open({
            type: 1,
            title: false, //不显示标题
            closeBtn: false,
            shadeClose: true,
            content: $('.recaptcha_box'), //捕获的元素，注意：最好该指定的元素要存放在body最外层，否则可能被其它的相对元素所影响
            cancel: function(){
                $('.recaptcha_box').hide();
            },
            end: function(){
                $('.recaptcha_box').hide();
            }
        })
    }
    var map,marker,marker1;
    var bill_inp_list = [];/*将要查询的运单号*/
    var thisLang = 'en-us'.split("-")[0];
    scrolltop('querypage_content');

    function func_search(billcode){
        toast.loading({
            title:"Loading",
            duration:2000
        });
        clientR.invoke({
            method: 'app.findTrack',
            data: {
                billcode: billcode,
                lang: thisLang,
                source: 3
            }
        }).then(function (ret,err) {
            ret = JSON.parse(ret);
            console.log(ret);
            $('.nogzcx_tip').show();
            if(!ret) return;
            if(ret.details.length>0){
                ret.details = ret.details.reverse();
                var html = '';
                var scantype = 0;
                var desctip = "";
                for(var i=0;i<ret.details.length;i++){
                    desctip = "";
                    var line1 = "Drop Point "+ ret.details[i].siteName;
                    var line3 = ret.details[i].scanstatus;
                    if(['2','3','4'].indexOf(ret.details[i].scanscode)>=0){
                        desctip = '<p>'+ret.details[i].desc+'</p>'
                    }
                    switch (ret.details[i].scanscode){
                        case '1':
                        case '4':
                            line3 = ret.details[i].scanstatus+" by " + ret.details[i].deliveryName;
                            break;
                        case '6':
                            line3 = line3+" "+ret.details[i].desc;
                            break;
                        case '5':
                        case '9':
                            line3 = ret.details[i].scanstatus+" to " + ret.details[i].signer;
                            break;
                        case '2':
                            if(ret.details[i].siteType=='网点'){
                                line1 = "Drop Point "+ ret.details[i].siteName;
                            }else{
                                line1 = "Transit Center";
                            }
                            line3 = "Departed to " + ret.details[i].nextSite;
                            break;
                        case '3':
                            if(ret.details[i].siteType=='网点'){
                                line1 = "Drop Point "+ ret.details[i].siteName;
                            }else{
                                line1 = "Transit Center";
                            }
                            break;
                        default :
                            break;
                    }
                    html += '<div class=\"bill_state_item\">'+
                    '<div class=\"bill_state_day\">'+
                    '<p>'+ret.details[i].scantime+'</p>'+
                    '</div>'+
                    '<div class=\"bill_state_text\">'+
                    '<p>'+line1+'</p>'+
                    "<p>City："+ret.details[i].city+"</p>"+
                        desctip+
                    '<p>'+line3+'</p>'+
                    '</div>'+
                    '</div>'
                }
                $('.bill_state_list').html(html);
                var sj = "Pick Up";
                var zc = "On Process";
                var pj = "On Delivery";
                var qs = "Delivered";
                switch (ret.details[ret.details.length-1].scanscode){
                    case '2':
                    case '3':
                        scantype = 1;
                        break;
                    case '4':
                        scantype = 2;
                        break;
                    case '6':
                    case '7':
                        scantype = 1;
                        var issj = false;
                        var istq = false;
                        for(var j=0;j<ret.details.length;j++){
                            if(ret.details[j].scanscode=='4'){
                                scantype = 2;
                            }else if(ret.details[j].scanscode=='2'){
                                issj = true;
                            }else if(ret.details[j].scanscode=='8'&&j==1){
                                istq = true;
                            }
                        }
                        scantype = issj?scantype:0;
                        scantype = istq?3:scantype;
                        if(scantype==2){
                            pj = "Difficulties";
                        }else if(scantype==1){
                            zc = "Difficulties";
                        }else if(scantype==3){
                            qs = "Difficulties";
                        }else{
                            sj = "Difficulties";
                        }
                        break;
                    case '5':
                        scantype = 3;
                        qs = ret.details[0].scantype;
                        break;
                    case '8':
                    case '9':
                        scantype = 3;
                        qs = ret.details[0].scanstatus;
                        break;
                    default :
                        scantype = 0;
                        break;
                }
                var htmlimg = '',showcalss;
                var dianhtml = '<div class=\"disn_line\"></div>';
                var showimg = (scantype==0&&(ret.details[0].scanscode=='6'||ret.details[0].scanscode=='7'))?'/static/images/on-hold.gif':'/static/images/pick-up.gif';
                htmlimg += '<span class=\"bill_state_img on\"> <img src='+showimg+' /> <span>'+sj+'</span> </span>';
                htmlimg += dianhtml;
                var showimg = (scantype==1&&(ret.details[0].scanscode=='6'||ret.details[0].scanscode=='7'))?'/static/images/on-hold.gif':'/static/images/on-progress.gif';
                if(scantype>=1){
                    showcalss = 'on';
                }else{
                    showimg ='/static/images/state1_back.png';
                }
                htmlimg += '<span class=\"bill_state_img '+showcalss+'\"> <img src='+showimg+' /> <span>'+zc+'</span> </span>';
                htmlimg += dianhtml;
                showimg = (scantype==2&&(ret.details[0].scanscode=='6'||ret.details[0].scanscode=='7'))?'/static/images/on-hold.gif':'/static/images/on-delivery.gif';
                if(scantype>=2){
                    showcalss = 'on';
                }else{
                    showimg ='/static/images/state2_back.png';
                }
                htmlimg += '<span class=\"bill_state_img '+showcalss+'\"> <img src='+showimg+' /> <span>'+pj+'</span> </span>';
                htmlimg += dianhtml;
                showimg = (scantype==3&&(ret.details[0].scanscode=='6'||ret.details[0].scanscode=='7'))?'/static/images/on-hold.gif':'/static/images/delivered.gif';
                if(scantype>=3){
                    showcalss = 'on';
                    if(ret.details[0].scanscode==8){
                        showimg = '/static/images/on_return.gif';
                    }else if(ret.details[0].scanscode==9){
                        showimg = '/static/images/returned.gif';
                    }
                }else{
                    showimg ='/static/images/state3_back.png';
                }
                htmlimg += '<span class=\"bill_state_img '+showcalss+'\"> <img src='+showimg+' /> <span>'+qs+'</span> </span>';
                $('.bill_state_listimg').html(htmlimg);
                $('.bill_state_has').show();
                $('.bill_state_null').hide();
                initialize(ret.details);
                scrolltop('bill_state_box');
            }else{
                layer.msg("Records not found", {icon: 5,time:layuimsgtime});
                $('.bill_state_has').hide();
                $('.bill_state_null').show();
            }
        },function(err){
            layer.msg("Query Failed", {icon: 5,time:layuimsgtime});
            $('.bill_state_has').hide();
            $('.bill_state_null').show();
            $('.nogzcx_tip').show();
        })
    }
    function searchGZ(state){
        var billthis = $('#billcode_list').val().toUpperCase().replace(/(^\s*)|(\s*$)/g,"");
        if((billthis.length>12||billthis.length<10)&&billthis){
            layer.msg("Maximum input 12 characters", {icon: 5,time:layuimsgtime});
            return;
        }
        if(bill_inp_list.length>0||billthis){
            if(notrobot&&!state){
                showrecaptcha();
            }else{
                expVerified();
                var billcode = billthis.replace(/\s|\,/g," ").split(" ");
                if(!billthis&&bill_inp_list.length<=0) return;
                billcode = billthis?bill_inp_list.concat(billcode):bill_inp_list;
                if(billcode.length>10){
                    layer.msg("Only ten data can be entered", {icon: 5,time:layuimsgtime});
                    return;
                }
                func_search(billcode[0]);

                $('.select_text span.text').text(billcode[0]);
                var billhtml = '';
                for(var j=0;j<billcode.length;j++){
                    billhtml += '<li>'+billcode[j]+'</li>'
                }
                $('.bill_list').html(billhtml);
                if(billthis==""&&bill_inp_list.length<=0){
                    $('.search_bill_list').hide();
                }else{
                    $('.search_bill_list').show();
                }
            }
        }else{
            layer.msg("Search content cannot be empty",{icon: 5,time:layuimsgtime});
        }
    }
    function inputbill(val,type){
        $('#billcode_list').val((val.match(/[a-zA-Z0-9]*/g)[0])||null);
        //监听文本框填写的内容
        if(bill_inp_list.length>=10){
            $('#billcode_list').val('');
            if(val!=" ") {
                layer.msg("Only ten data can be entered", {icon: 5,time:layuimsgtime});
            }
            return;
        }
        if(/\s|\,/g.test(val)){
            if((val.length>13||val.length<11)&&!type){
                layer.msg("Maximum input 12 characters", {icon: 5,time:layuimsgtime});
                $('#billcode_list').val(val.replace(/\s|\,/g,""));
                return;
            }
            var lslist = val.replace(/\s|\,/g," ").split(" ");
            for(var i=0;i<lslist.length;i++){
                if(lslist[i]){
                    var arritem = lslist[i].toUpperCase();
                    bill_inp_list.push(arritem);
                    var bhtml = '<span class="inp_bill_item">'+arritem+' <i class="iconfont icon-fork"></i></span>';
                    $('#billcode_list').width('250').attr('placeholder',"Please enter the waybill number").before(bhtml);
                }
            }
            $('#billcode_list').val('').focus();
        }
    }
    var bills = "";
    if(bills){
        inputbill(bills,true);
        searchGZ(true);
    }

/*切换明细与地图*/
    $('.cx_tap_btn1').click(function(){
        $('.bill_state_list').show();
        $('.cx_tap_btn1').addClass('on');
        $('.googleMap').hide();
        $('.cx_tap_btn2').removeClass('on');
    });
    $('.cx_tap_btn2').click(function(){
        $('.bill_state_list').hide();
        $('.cx_tap_btn2').addClass('on');
        $('.googleMap').show();
        $('.cx_tap_btn1').removeClass('on');
    });
/*切换明细与地图end*/

/*切换查询的运单号*/
    $(document).on('click','.search_bill_item',function(){
        var bill = $(this).text();
        $('.search_bill_item').removeClass('on');
        $(this).addClass('on');
        func_search(bill);
    })
/*切换查询的运单号end*/
    $('.inp_val_list').click(function(){
        /*清空文本框*/
        $('#billcode_list').val('');
        $('.inp_bill_item').remove();
        bill_inp_list = [];
        $('#billcode_list').width('').attr('placeholder'," Please enter your waybill number. Available up to 10 waybills.");
    })
    $('.bill_inp').on('click','.inp_bill_item .icon-fork',function(){
        /*删除单条文本数据*/
        $(this).parent().remove();
        bill_inp_list.splice($(this).parent().index(),1);
        if(bill_inp_list.length<=0){
            $('#billcode_list').width('').attr('placeholder'," Please enter your waybill number. Available up to 10 waybills.");
        }
    })

/*运单选择的下拉框打开与关闭*/
$('.select_text').on('click',function(){
    $('.bill_list').slideToggle();
    $('.select_text').toggleClass('on');
})
$(document).on('click','.bill_list li',function(){
    var txt = $(this).text();
    $('.select_text span.text').text(txt);
    $('.bill_list').slideUp();
    $('.select_text').removeClass('on');
    func_search(txt);
})
//其他元素被点击则收起选择
$(document).on('mousedown', function(e){
    closeSelect(e);
});
$(document).on('touchstart', function(e){
    closeSelect(e);
});

function closeSelect(e) {
    var bill_list = $('.search_bill_list');
    var filter = bill_list.find('.bill_list'),
        filterEl = bill_list.find('.bill_list')[0];
    var filterBoxEl = bill_list[0];
    var target = e.target;
    if(filterEl !== target && !$.contains(filterEl, target) && !$.contains(filterBoxEl, target)) {
        $('.bill_list').slideUp();
        $('.select_text').removeClass('on');
    }
}
/*运单选择的下拉框打开与关闭end*/

    function initialize(data)
    {
        return;
        var lat = data[0].latitude?data[0].latitude:'14.32585';
        var lon = data[0].longitude?data[0].longitude:'121.02448';
        var lat1 = data[data.length-1].latitude?data[data.length-1].latitude:'14.32585';
        var lon1 = data[data.length-1].longitude?data[data.length-1].longitude:'121.02448';
        var Center = new google.maps.LatLng(lat,lon);
        var Destination = new google.maps.LatLng(lat1,lon1);
        var mapProp = {
            center:Center,
            zoom:9,
            mapTypeId:google.maps.MapTypeId.ROADMAP
        };
        map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
        marker = new google.maps.Marker({
            position: Center,
            map: map
        });
        if(data[0].latitude=="") return;
        var mapstis = "<div>"+data[0].city+"</div>";
        var infowindow = new google.maps.InfoWindow({content:mapstis});
        infowindow.open(map,marker);

        if(data.length<=1||data[data.length-1].latitude=="") return;
        marker1 = new google.maps.Marker({
            position: Destination,
            map: map
        });
        var mapstis1 = "<div>"+data[data.length-1].city+"</div>";
        var infowindow1 = new google.maps.InfoWindow({content:mapstis1});
        infowindow1.open(map,marker1);

        var flightPath = new google.maps.Polyline({
            path: [Center,Destination],
            geodesic: true,
            strokeColor: '#dc3232',
            strokeOpacity: 1.0,
            strokeWeight: 2
        });
        flightPath.setMap(map);

        marker.addListener('click', function() {
            infowindow.open(map, marker);
        });
        marker1.addListener('click', function() {
            infowindow1.open(map, marker1);
        });
        markershow = true;
    }

    function keyEnter(){
        if (event.keyCode==13){
            var billtext = $('#billcode_list').val();
            if(billtext){
                inputbill(billtext+' ')
            }
        }  //回车键的键值为13
    }
    function billdel(){
        if (event.keyCode==8){
            if($('#billcode_list').is(':focus')&&$('#billcode_list').val()==""){
                /*删除单条文本数据*/
                if(bill_inp_list.length>0){
                    $('#billcode_list').prev().remove();
                    bill_inp_list.splice(bill_inp_list.length-1,1);
                }
            }
        }
    }

    //切换验证码
    $('#imgVercode,.shuaxing_btn').click(function () {
        $('#imgVercode').attr('src', '/captcha.html');
        $("input[name='webyzm']").val('');
    });

    function keyyz(){
        if (event.keyCode==13)  //回车键的键值为13
            robotVerified()
    }
