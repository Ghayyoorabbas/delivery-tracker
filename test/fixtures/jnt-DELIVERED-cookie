    var toast = new auiToast();
    $(document).on('focus','.form_item input,.reg_inp input',function(){
        $(this).parent().addClass('on');
    });
    $(document).on('blur','.form_item input,.reg_inp input',function(){
        if($(this).val()==''){
            $(this).parent().removeClass('on')
        }
    });
    $(document).on('input','.reg_inp input',function(){
        $(this).parent('.reg_inp').removeClass('error_b').find('.error_desc').remove();
        var vals = $(this).val();
        var deschtml = '',isEmail='';
        switch ($(this).attr('id')){
            case ('userName'):
                if(!vals){
                    deschtml = "<span class='error_desc'>"+"Username can not be empty"+"</span>";
                    $('#userName').parent().append(deschtml).addClass('error_b');
                }else{
                    if(vals.length>30||vals.length<3){
                        deschtml = "<span class='error_desc'>"+"Must enter 3-30 characters"+"</span>";
                        $('#userName').parent().append(deschtml).addClass('error_b');
                    }
                }
                break;
            case ('phonenumber'):
                if(!vals){
                    deschtml = "<span class='error_desc'>"+"Phone number can't be empty"+"</span>";
                    $('#phonenumber').parent().append(deschtml).addClass('error_b');
                }else{
                    isEmail = /^[\+\d]{10,15}/;
                    if(!(isEmail.test(vals))){
                        deschtml = "<span class='error_desc'>"+"Must enter 10-15 digits"+"</span>";
                        $('#phonenumber').parent().append(deschtml).addClass('error_b');
                    }
                }
                break;
            case ('mailbox'):
                if(!vals){
                    deschtml = "<span class='error_desc'>"+"Mailbox can not be empty"+"</span>";
                    $('#mailbox').parent().append(deschtml).addClass('error_b');
                }else{
                    isEmail = /^([a-zA-Z0-9\._-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
                    if(!(isEmail.test(vals))){
                        deschtml = "<span class='error_desc'>"+"E-mail format is incorrect"+"</span>";
                        $('#mailbox').parent().append(deschtml).addClass('error_b');
                    }
                }
                break;
            case ('password'):
                if(!vals){
                    deschtml = "<span class='error_desc'>"+"Password Require"+"</span>";
                    $('#password').parent().append(deschtml).addClass('error_b');
                }else{
                    if(vals.length>20||vals.length<6){
                        deschtml = "<span class='error_desc'>"+"6-20 characters must be entered"+"</span>";
                        $('#password').parent().append(deschtml).addClass('error_b');
                    }
                }
                break;
            case ('frenewpwd'):
                if($('#password').val()!=vals){
                    deschtml = "<span class='error_desc'>"+"The passwords must be the same twice"+"</span>";
                    $('#frenewpwd').parent().append(deschtml).addClass('error_b');
                }
                break;
            case ('for_mailbox'):
                if(!vals){
                    deschtml = "<span class='error_desc'>"+"Mailbox can not be empty"+"</span>";
                    $('#for_mailbox').parent().append(deschtml).addClass('error_b');
                }else{
                    isEmail = /^([a-zA-Z0-9\._-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
                    if(!(isEmail.test(vals))){
                        deschtml = "<span class='error_desc'>"+"E-mail format is incorrect"+"</span>";
                        $('#for_mailbox').parent().append(deschtml).addClass('error_b');
                    }
                }
                break;
        }
    })
    $(document).on('focus','.yzm_item input',function(){
        $(this).parent().addClass('on');
    })
    $(document).on('blur','.yzm_item.on input',function(){
        $(this).parent().removeClass('on');
    })
    $(document).on('input','.yzm_item input',function(){
        var val = $(this).val();
        var inptext = val.substr(val.length-1,1);
        var num = $(this).attr('data-num');
        if(inptext){
            $(this).val(inptext);
            $(this).prev().text(inptext);
            if(num==6){
                var user_yzmbox = $(this).parents(".reg_inp").attr("data-name");
                if(user_yzmbox=="user_yzmbox"){
                    JYcode_info();
                }else{
                    JYcode();
                }
            }else{
                $('.yzm_item').removeClass('on').find('input').attr('disabled','disabled');
                $(this).parent().next().addClass('on').find('input').removeAttr('disabled').focus();
            }
        }else{
            if(num==6) true;
            $(this).val(inptext);
            $(this).prev().text(inptext);
        }
    })
    $(document).on('click','.btn_upclose',function(){
        $('.fixed_forgot').removeClass('reg_show');
        $(".login_bg").hide();
    })
    function keydel(){
        if (event.keyCode==8){
            if($('.yzm_item').hasClass('on')){
                var inptext = $('.yzm_item.on input').val();
                var num = $('.yzm_item.on input').attr('data-num');
                if(!inptext&&num>1){
                    $('.yzm_item.on').prev().addClass('on').find('input').val('').removeAttr('disabled').focus();
                    $('.yzm_item.on').find('span').text('');
                    $('.yzm_item').not('.on').removeClass('on').find('input').attr('disabled','disabled');
                }
            }
        }
    }
    function jumpOrder(url){
        var islogin = '';
        if(!islogin){
            $('.nav_list').removeClass('on');
            if($('.index_login').hasClass('login_show')){
                $('.register_box .close').trigger("click");
            }else{
                $('.index_login').addClass('login_show');
                $('.login_bg').show();
                $('.fixed_register').removeClass('reg_show');
                $('.protocol_box').removeClass('on');
                $('.fixed_forgot').removeClass('reg_show');
            }
            layer.msg("please log in first",{time:layuimsgtime});
            return;
        }
        window.location.href = url;
    }
    var loginType = '1';//1，手机号登录，2：用户名邮箱登录
    $('#Login').click(function(){
        $('.form_item').removeClass('error_b').find('.error_desc').remove();
        var formData = {},fnama,loginmode;
        var deschtml = '';
        var f = $('form#form_login').serializeArray();
        $.each(f, function() {
            formData[this.name] = this.value;
        });
        loginTo(loginType,formData);
    })

    //sessionid获取成功，查询用户信息
    function login_ok(ret0,formData){
        if(ret0.code==200){
            ret0.data = JSON.parse(ret0.data);
            clientR.invoke({
                method: 'cus.getUserProfile',
                data: "",
                sessionid: ret0.data.sessionid
            }).then(function (ret,err) {
                switch (ret.code){
                    case 200:
                        if(formData){
                            if($('#remember').is(':checked')){
                                var rememberData={
                                    name: formData.flog_name,
                                    phone: formData.flog_phone,
                                    password: formData.fpwd
                                }
                                localStorage.setItem("rememberData",JSON.stringify(rememberData));
                            }else{
                                localStorage.removeItem("rememberData");
                            }
                        }
                        ret.data = JSON.parse(ret.data);
                        if(!ret.data.username){
                            ret.data.username = ret.data.name;
                        }
                        ret.data.usernickname = ret.data.name;
                        ret.data.phonenumber = ret.data.phoneNumber?ret.data.phoneNumber:"";
                        ret.data.cuscity = ret.data.city?ret.data.city:"";
                        ret.data.mailbox = ret.data.email?ret.data.email:'';
                        ret.data.userName = ret.data.username;
                        ret.data.cusaddress = ret.data.address?ret.data.address:"";
                        ret.data.userCode = ret.data.id;
                        ret.data.cuspostcode = ret.data.postcode?ret.data.postcode:'';
                        ret.data.sessionid = ret0.data.sessionid;
                        clientPC.invoke('user_sessionInfo', [ret.data]).then(function (ret) {
                            window.location.href = "/index/order/myorder.html";
                        });
                        break;
                    case 4170:
                        layer.msg("授权令牌无效", {icon: 5,time:layuimsgtime});
                        break;
                    default :
                        layer.msg("System Error", {icon: 5,time:layuimsgtime});
                        break;
                }
            })
        }else if(ret0.code==414){
            layer.msg("Incorrect username or password", {icon: 5,time:layuimsgtime});
        }else{
            layer.msg("System Error", {icon: 5,time:layuimsgtime});
        }
    }
    function keyLogin(){
        if (event.keyCode==13)  //回车键的键值为13
            $("#Login").click(); //调用登录按钮的登录事件
    }
    //执行登录接口获取sessionid
    function loginTo(ltype,formData){
        var fnama,loginmode;
        toast.loading({
            title:"Login",
            duration:2000
        });
        if(ltype=='1'){
            fnama = formData.flog_phone.replace(/^(63\-|63|\+63|0)/,"");
            loginmode = '1';
        }else{
            var isEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
            if(!(isEmail.test(formData.mailbox))){
                loginmode = '2'
            }else{
                loginmode = '3'
            }
            fnama = formData.flog_name;
        }
        if(!fnama||!formData.fpwd){
            layer.msg("Please enter account and password",{time:layuimsgtime});
            toast.hide()
            return;
        }
        clientR.invoke({
            method: 'cus.login',
            data: {
                account: fnama,
                areaCode: '63',
                password: formData.fpwd,
                loginType: loginmode
            }
        }).then(function (ret0,err) {
            login_ok(ret0,formData)
        })
    }
    $('.logout').click(function(){
        clientPC.invoke('user_sessionOut', []).then(function (ret) {
            location.reload();
        });
    })
    $('.login_title>div').click(function(){
        var index = $(this).index();
        $('.login_title>div').removeClass('on').eq(index).addClass('on');
        if(index==0){
            loginType = 1;
            $('#namemail_box').hide();
            $('#phone_box').show();
        }else{
            loginType = 2;
            $('#namemail_box').show();
            $('#phone_box').hide();
        }
    })

    var regData = {},djs ='',djs_reg ='',times = 120,times_reg = 120,getok=false,getok_reg=false,for_phonenumber='',for_yzmbox='',for_phoneqh='';
    $('#submit1').click(function(){
        var btnnum = $(this).attr('id');
        if(!$('#checkxy').is(':checked')){
            layer.alert("Please read the User Agreement carefully and agree to it", {icon: 2,closeBtn:0,title:"",btn:["Got it"]});
            return;
        }
        $('.reg_inp').removeClass('error_b').find('.error_desc').remove();
        var deschtml = '';
        var formData = {};
        var f = $('.fixed_register form.register_cont').serializeArray();
        $.each(f, function() {
            formData[this.name] = this.value;
        });
        if(!formData.userName){
            deschtml = "<span class='error_desc'>"+"Username can not be empty"+"</span>";
            $('#userName').parent().append(deschtml).addClass('error_b');
            return;
        }
        if(!formData.phonenumber){
            deschtml = "<span class='error_desc'>"+"Phone number can't be empty"+"</span>";
            $('#phonenumber').parent().append(deschtml).addClass('error_b');
            return;
        }else{
            var pnum = /^(09)[\d]{9}$/;
            if(!(pnum.test(formData.phonenumber))){
                layer.msg("The number format is not correct", {icon: 5,time:layuimsgtime});
                return;
            }
        }
        if(!formData.mailbox){
            deschtml = "<span class='error_desc'>"+"Mailbox can not be empty"+"</span>";
            $('#mailbox').parent().append(deschtml).addClass('error_b');
            return;
        }else{
            var isEmail = /^([a-zA-Z0-9\._-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
            if(!(isEmail.test(formData.mailbox))){
                deschtml = "<span class='error_desc'>"+"E-mail format is incorrect"+"</span>";
                $('#mailbox').parent().append(deschtml).addClass('error_b');
                return;
            }
        }
        var ispwd = /^[a-zA-Z0-9]{6,20}$/;
        if(!(ispwd.test(formData.password))){
            deschtml = "<span class='error_desc'>"+"The password must be a combination of letters or numbers and cannot be less than 6-20 digits"+"</span>";
            $('#password').parent().append(deschtml).addClass('error_b');
            return;
        }
        if(!formData.password){
            deschtml = "<span class='error_desc'>"+"Password Require"+"</span>";
            $('#password').parent().append(deschtml).addClass('error_b');
            return;
        }
        if(formData.password!=formData.frenewpwd){
            deschtml = "<span class='error_desc'>"+"The passwords must be the same twice"+"</span>";
            $('#frenewpwd').parent().append(deschtml).addClass('error_b');
            return;
        }
        var phonenum = formData.phonenumber.replace(/^(0|63|\+63)/,"");
        regData = {
            username: formData.userName,
            areaCode: "63",
            securityCode: "",
            phoneNumber: phonenum,
            email: formData.mailbox,
            password: formData.password,
            name: formData.userName,
            city: "",
            address: "",
            postcode: ""
        }
        toast.loading({
            title:"get code",
            duration:2000
        });
        var method,dataparameter;
        method = 'securityCode.register';
        dataparameter = {
            phoneNumber: phonenum,
            areaCode: "63",
            lang: "en"
        }
        clientR.invoke({
            method: method,
            data: dataparameter
        }).then(function (ret,err) {
            switch (ret.code){
                case 200:
                    ret.data = JSON.parse(ret.data);
                    getok_reg = true;
                    times_reg = ret.data.timeDiff*1;
                    clearInterval(djs_reg);
                    djs_reg = setInterval(function(){
                        times_reg--;
                        if(times_reg>=0){
                            $('.get_yzm_reg').text(times_reg+'s')
                        }else{
                            getok_reg = false;
                            clearInterval(djs_reg);
                            $('.get_yzm_reg').text("get code")
                        }
                    },1000);
                    $('.my_reg_yzm').show().html(inpyzm+"<div style='text-align: center;font-size: 14px; margin-top: 20px;'>请输入验证码后自动注册</div><div class='get_yzm_reg text_center aui-font-size-16 col_them'></div>");
                    $('.myreg_content').hide();
                    break;
                case 410:
                    layer.msg("Cell phone number already exists", {icon: 5,time:layuimsgtime});
                    break;
            }
        },function(err){
            layer.msg("获取失败", {icon: 5,time:layuimsgtime});
        });
    })
    $('#xyshow').click(function(){
//        window.open("<?//=url('../index/mailpwd/agreement', '',false,true)?>//","_blank");
//        $('.protocol_box').addClass('on');
    })
