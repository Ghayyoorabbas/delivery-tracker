    function JYcode()
    {
        if($('.fixed_register').hasClass('reg_show')){
            var textyzm = $('#register #for_yzmbox_1').val()+$('#register #for_yzmbox_2').val()+$('#register #for_yzmbox_3').val()+$('#register #for_yzmbox_4').val()+$('#register #for_yzmbox_5').val()+$('#register #for_yzmbox_6').val();
            toast.loading({
                title:"Register",
                duration:2000
            });
            clientR.invoke({
                method: 'cus.verifySecurityCode',
                data: {
                    phoneNumber: regData.phoneNumber,
                    areaCode: "63",
                    securityCode: textyzm
                }
            }).then(function (ret,err) {
                switch (ret.code){
                    case 200:
                        toast.loading({
                            title:"Register",
                            duration:2000
                        });
                        regData.securityCode = textyzm;
                        clientR.invoke({
                            method: 'cus.register',
                            data: regData
                        }).then(function (ret,err) {
                            switch (ret.code){
                                case 200:
                                    loginTo('1',{
                                        flog_phone: regData.phoneNumber,
                                        fpwd: regData.password
                                    })
                                    break;
                                case 410:
                                    deschtml = "<span class='error_desc'>"+"Cell phone number already exists"+"</span>";
                                    $('#phonenumber').parent().append(deschtml);
                                    break;
                                case 411:
                                    layer.msg("Verification failed", {icon: 5,time:layuimsgtime});
                                    break;
                                case 412:
                                    deschtml = "<span class='error_desc'>"+"Username already exists"+"</span>";
                                    $('#userName').parent().append(deschtml);
                                    break;
                                case 413:
                                    deschtml = "<span class='error_desc'>"+"Mailbox already exists"+"</span>";
                                    $('#mailbox').parent().append(deschtml);
                                    break;
                            }
                            $('.my_reg_yzm').html("");
                            $('.myreg_content').show();
                            regData = {};
                        },function(err){
                            layer.msg("Registration failed", {icon: 5,time:layuimsgtime});
                            $('.my_reg_yzm').html("");
                            $('.myreg_content').show();
                            regData = {};
                        });
                        break;
                    default:
                        layer.msg("Verification failed", {icon: 5,time:layuimsgtime});
                }
            },function(err){
                layer.msg("Verification failed", {icon: 5,time:layuimsgtime});
            });
        }
    }
    $(document).on('click','#UPpwd',function(){
        $('.error_desc').remove();
        var formData = {},deschtml='';
        var f = $('.fixed_forgot form#jycode').serializeArray();
        var phonenumber = $('#for_phonenumber').val();
        $.each(f, function() {
            formData[this.name] = this.value;
        });
        var ispwd = /^[a-zA-Z0-9]{6,20}$/;
        if(!(ispwd.test(formData.for_password))){
            layer.msg("The password must be a combination of letters or numbers and cannot be less than 6-20 digits", {icon: 5,time:layuimsgtime});
            return;
        }
        if(!formData.for_password){
            layer.msg("Password Require", {icon: 5,time:layuimsgtime});
            return;
        }
        if(formData.for_password!=formData.for_renewpwd){
            layer.msg("The passwords must be the same twice", {icon: 5,time:layuimsgtime});
            return;
        }
        var pnum = /^(09)[\d]{9}$/;
        if(!pnum.test(phonenumber)){
            layer.msg("The number format is not correct", {icon: 5,time:layuimsgtime});
            return;
        }
        var textyzm = $('#jycode #for_yzmbox_1').val()+$('#jycode #for_yzmbox_2').val()+$('#jycode #for_yzmbox_3').val()+$('#jycode #for_yzmbox_4').val()+$('#jycode #for_yzmbox_5').val()+$('#jycode #for_yzmbox_6').val();
        toast.loading({
            title:"Change Password",
            duration:2000
        });
        clientR.invoke({
            method: 'cus.changePasswordByMsg',
            data: {
                phoneNumber: phonenumber.replace(/^(0|63|\+63)/,""),
                areaCode: "63",
                password: formData.for_password,
                securityCode: textyzm
            }
        }).then(function (ret,err) {
            switch (ret.code){
                case 200:
                    $('.fixed_forgot').removeClass('reg_show');
                    $('.login_bg').hide();
                    $('.person_box,.personal_item').trigger('click');
                    layer.msg("Reset password successfully", {icon: 6,time:layuimsgtime});
                    break;
                default:
                    layer.msg("Vercode Incorrect", {icon: 5,time:layuimsgtime});
                    break;
            }
        },function(err){
            layer.msg("Failed to reset password", {icon: 5,time:layuimsgtime});
        });
    })
    //忘记密码重新获取验证码
    $(document).on('click','.get_yzm',function(){
        if(getok) return;
        $('.error_desc').remove();
        var deschtml = '';
        var phonenumber = $('#for_phonenumber').val();
        var phoneqh = $('#for_phoneqh').val();
        if(!phonenumber){
            layer.msg("Phone number can't be empty", {icon: 5,time:layuimsgtime});
            return;
        }else{
            var pnum = /^(09)[\d]{9}$/;
            if(!(pnum.test(phonenumber))){
                layer.msg("The number format is not correct", {icon: 5,time:layuimsgtime});
                return;
            }
        }
        toast.loading({
            title:"get code",
            duration:2000
        });
        var phonenum = phonenumber.replace(/^(0|63|\+63)/,"");
        clientR.invoke({
            method: 'securityCode.updtePassword',
            data: {
                lang: "en",
                areaCode: "63",
                phoneNumber: phonenum
            }
        }).then(function (ret,err) {
            switch (ret.code){
                case 200:
                    ret.data = JSON.parse(ret.data);
                    getok = true;
                    times = ret.data.timeDiff;
                    clearInterval(djs);
                    djs = setInterval(function(){
                        times--;
                        if(times>=0){
                            $('.get_yzm').text(times+'s')
                        }else{
                            getok = false;
                            clearInterval(djs);
                            $('.get_yzm').text("get code")
                        }
                    },1000);
                    break;
                case 415:
                    layer.msg("Mobile number does not exist", {icon: 5,time:layuimsgtime});
                    break;
                case 420:
                    ret.data = JSON.parse(ret.data);
                    getok = true;
                    times = ret.data.timeDiff;
                    clearInterval(djs);
                    djs = setInterval(function(){
                        times--;
                        if(times>=0){
                            $('.get_yzm').text(times+'s')
                        }else{
                            getok = false;
                            clearInterval(djs);
                            $('.get_yzm').text("get code")
                        }
                    },1000);
                    layer.msg("Too often", {icon: 5,time:layuimsgtime});
                    break;
                default:
                    layer.msg("获取失败", {icon: 5,time:layuimsgtime});
                    break;
            }
        },function(err){
            layer.msg("获取失败", {icon: 5,time:layuimsgtime});
        });
    })
    //注册重新获取验证码
    $(document).on('click','.get_yzm_reg',function(){
        if(getok_reg) return;
        $('#submit1').click();
        return;
        toast.loading({
            title:"get code",
            duration:2000
        });
        clientR.invoke({
            method: 'website.getVerifyCode',
            data: {
                lang: "en",
                areaCode: "63",
                phoneNumber: regData.phoneNumber
            }
        }).then(function (ret,err) {
            switch (ret.code){
                case 200:
                    ret.data = JSON.parse(ret.data);
                    getok_reg = true;
                    times_reg = ret.data.timeDiff*1;
                    clearInterval(djs_reg);
                    djs_reg = setInterval(function(){
                        times_reg--;
                        if(times_reg>=0){
                            $('.get_yzm_reg').text(times_reg+'s')
                        }else{
                            getok_reg = false;
                            clearInterval(djs_reg);
                            $('.get_yzm_reg').text("get code")
                        }
                    },1000);
                    $('.my_reg_yzm').show().html(inpyzm+"<div style='text-align: center;font-size: 14px; margin-top: 20px;'>请输入验证码后自动注册</div><div class='get_yzm_reg text_center aui-font-size-16 col_them'></div>");
                    $('.myreg_content').hide();
                    break;
                case 410:
                    layer.msg("Cell phone number already exists", {icon: 5,time:layuimsgtime});
                    break;
            }
        },function(err){
            layer.msg("获取失败", {icon: 5,time:layuimsgtime});
        });
    })
    /*邮箱修改密码*/
    $('#UPMail').click(function(){
        var mailval = $('#for_mailbox').val();
        if(!mailval){
            layer.msg("Mailbox can not be empty", {icon: 5,time:layuimsgtime});
            return;
        }else{
            var isEmail = /^([a-zA-Z0-9\._-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
            if(!(isEmail.test(mailval))){
                layer.msg("E-mail format is incorrect", {icon: 5,time:layuimsgtime});
                return;
            }
        }
        toast.loading({
            title:"Sending",
            duration:2000
        });
        clientR.invoke({
            method: 'email.sendEmail',
            data: {
                emailAddress: mailval,
                lang: "en"
            }
        }).then(function (ret,err) {
            switch (ret.code){
                case 200:
//                    clientPC.invoke('mailpwd_add', [{mail: mailval}]).then(function (ret) {});
                    $('.fixed_forgot').removeClass('reg_show');
                    $('.login_bg').hide();
                    $('#for_mailbox').val('');
                    layer.msg("Please check your emails", {icon: 6,time:layuimsgtime});
                    break;
                case 415:
                    layer.msg("Mailbox does not exist", {icon: 5,time:layuimsgtime});
                    break;
                default :
                    layer.msg("Failed to send", {icon: 5,time:layuimsgtime});
                    break;
            }
        },function(err){
            layer.msg("Failed to send", {icon: 5,time:layuimsgtime});
        });
    })
    $(document).on('click','.isee',function(){
        if($(this).hasClass('open')){
            $(this).removeClass('open').attr('src','/static/images/open_isee.png');
            $(this).prev('.isisee').attr('type','text');
        }else{
            $(this).addClass('open').attr('src','/static/images/close_isee.png');
            $(this).prev('.isisee').attr('type','password');
        }
    })
    window.onload = function(){
        $('.index_login input,.fixed_register input,.fixed_forgot input').val('');
    }
</script>



