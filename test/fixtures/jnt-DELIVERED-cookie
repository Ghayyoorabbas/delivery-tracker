<script>
    $.getScript("https://apis.google.com/js/api:client.js",function (res) {
        startApp();
    });
    var googleUser = {};
    var startApp = function () {
        gapi.load('auth2', function () {
            auth2 = gapi.auth2.init({
                //726009858027-1fuqkvl234ehdode55mhr90msp9ovuvt.apps.googleusercontent.com
                client_id: '1697724314-34c20htv4udm9q14sl6pa6umpah4kpv9.apps.googleusercontent.com', //客户端ID
                cookiepolicy: 'single_host_origin',
                scope: 'profile' //可以请求除了默认的'profile' and 'email'之外的数据
            });
            if(document.getElementById('customBtn')){
                attachSignin(document.getElementById('customBtn'));
            }
        });
    };
    function attachSignin(element) {
        auth2.attachClickHandler(element, {},
            function (googleUser) {
                toast.loading({
                    title:"Login",
                    duration:2000
                });
                var profile = auth2.currentUser.get().getBasicProfile();
                var id_token = googleUser.getAuthResponse().id_token;
                console.log(googleUser.getAuthResponse())
                var userId = profile.getId();
                var FullName = profile.getName();
                var GivenName = profile.getGivenName();
                var FamilyName = profile.getFamilyName();
                var Email = profile.getEmail();
                var ImageURL = profile.getImageUrl();
                var data = {
                    userId: userId,
                    Headpic: ImageURL,
                    FullName: FullName,
                    GivenName: GivenName,
                    FamilyName: FamilyName,
                    Email: Email,
                    loginTye: 'google+'
                };
                if (userId > 0) {
                    console.log(profile,data)
                    clientR.invoke({
                        method: 'third.googleWebLogin',
                        data: {
                            id_token: id_token,
                            code: userId,
                            userName: FullName
                        }
                    }).then(function (ret0,err) {
                        toast.hide();
                        if(ret0.code==200){
                            login_ok(ret0)
                        }else{
                            layer.msg(ret0.code+" Login failed", {icon: 5,time:layuimsgtime});
                        }
                    })
                }else{
                    toast.hide();
                    layer.msg("Login failed", {icon: 5,time:layuimsgtime});
                }
            },
            function (error) {
                console.log(JSON.stringify(error, undefined, 2));
            });
    }

    //注销
    function signOut() {
        var auth2 = gapi.auth2.getAuthInstance();
        auth2.signOut().then(function () {
            alert('用户注销成功');
        });
    }
</script>
